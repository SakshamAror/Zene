{
  "expo": {
    "name": "Zene",
    "slug": "zene-mindfulness-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffdf7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zene.mindfulness"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fffdf7"
      },
      "package": "com.zene.mindfulness"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "9ffaa7d0-9b96-4c82-a82c-9ce65558eb87"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "zene"
  }
}